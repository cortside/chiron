{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Seq",
                "Args": { "serverUrl": "http://seq:5341" }
            },
            {
                "Name": "RollingFile",
                "Args": { "pathFormat": "Logs/{Date}.log" }
            }
        ]
    }
}
